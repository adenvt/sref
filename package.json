{
  "name": "sref",
  "packageManager": "yarn@4.2.2",
  "description": "Super small, bare minimum reactive variable",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/sref.umd.cjs",
  "module": "./dist/sref.js",
  "types": "./dist/sref.d.ts",
  "exports": {
    ".": {
      "require": "./dist/sref.umd.cjs",
      "import": "./dist/sref.js",
      "types": "./dist/sref.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepack": "yarn build",
    "lint": "eslint . --ext .js,.vue,.ts --format pretty",
    "fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@privyid/eslint-config-persona": "^1.0.0-rc.7",
    "@types/eslint": "^8",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-v8": "^2.0.4",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint": "8.57.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-formatter-pretty": "5.0.0",
    "eslint-plugin-align-assignments": "1.1.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-varspacing": "1.2.2",
    "eslint-plugin-vue": "9.23.0",
    "typescript": "5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^4.0.0-beta.1",
    "vitest": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adenvt/sref.git"
  }
}
